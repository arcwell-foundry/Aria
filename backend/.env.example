# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Anthropic Claude API
ANTHROPIC_API_KEY=sk-ant-your-api-key

# Neo4j (Graphiti) Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password

# Tavus (Phase 6)
TAVUS_API_KEY=your-tavus-api-key

# Daily.co (Phase 6)
DAILY_API_KEY=your-daily-api-key

# Composio OAuth
COMPOSIO_API_KEY=your-composio-api-key
# Composio Base URL (optional, defaults to https://api.composio.dev)
# COMPOSIO_BASE_URL=https://api.composio.dev

# Application Settings
APP_SECRET_KEY=your-secret-key-at-least-32-characters-long
APP_ENV=development

# CORS (comma-separated list of allowed origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Email Service (Resend)
RESEND_API_KEY=your-resend-api-key

# Billing (Stripe)
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Web Search & Enrichment (Exa)
EXA_API_KEY=your-exa-api-key
